{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "html/**", "use": "@vercel/static" },
    { "src": "Healthyfy/frontend/html/**", "use": "@vercel/static" },
    { "src": "css/**", "use": "@vercel/static" },
    { "src": "js/**", "use": "@vercel/static" },
    { "src": "assets/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "^/healthyfy$", "dest": "/Healthyfy/frontend/html/index.html" },
    { "src": "^/healthyfy/(.*)$", "dest": "/Healthyfy/frontend/html/$1" },
    { "src": "^/healthyfy/(.*)$", "has": [{ "type": "query", "key": "_escaped_fragment_" }], "dest": "/Healthyfy/frontend/html/index.html" },
    { "src": "^/(.*)$", "dest": "/index.html" }
  ]
}

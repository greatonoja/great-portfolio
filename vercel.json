{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "html/**", "use": "@vercel/static" },
    { "src": "Healthyfy/frontend/html/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/healthyfy/(.*)", "dest": "/Healthyfy/frontend/html/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
